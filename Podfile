  pod 'SDWebImage', '~> 4.4.2'
  pod 'SimpleImageViewer', '~> 1.1.1'
  pod 'SwiftyJSON', '~> 4.2.0'
  pod 'Alamofire', '~> 4.7.3'
  
end
